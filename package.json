{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean-imports": "tsx clean-import-extensions.ts",
    "ingest:kb": "tsx scripts/ingestKnowledge.ts",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --watch src --ext ts --exec tsx src/server.ts",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@notionhq/client": "^2.3.0",
    "@supabase/supabase-js": "^2.49.4",
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yamljs": "^0.3.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "concurrently": "^8.2.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "globby": "^14.1.0",
    "jsdom": "^24.1.3",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "ts-node": "^10.9.1",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "vite": "^6.3.0",
    "vitest": "^3.1.1",
    "zod-to-ts": "^1.2.0"
  }
}
