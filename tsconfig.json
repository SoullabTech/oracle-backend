  "compilerOptions": {
    "target": "ES6", 
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.ts",
    "src/flows/**/*.*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "uploads"
  ]
}
